            } */
        }

        @media (min-width: 1024px) {
            .star-watermark {
                top: 35vh;
                left: 27vw;
                height: 80vh;
            }

            .logo {
                height: 4.5rem;
                margin-bottom: 1.5rem;
                margin-top: 1.5rem;
            }

            main {
                width: 85%;
            }

            .hero {
                height: 40vh;
            }

            h1 {
                margin-top: 10px;
                font-size: 3rem;
            }

            footer {
                padding: 1.5rem;
                border-bottom-left-radius: 0;
                border-bottom-right-radius: 0;
            }

            footer>img {
                top: -4.8rem;
                left: -4rem;
                width: 22vw;
            }

            /* footer .logo-container {
        background-color: red;
      } */

            footer .flogo {
                width: 12rem;
                /* height: auto; */
            }

            footer .top .middle {
                display: flex;
                align-items: center;
                gap: 1.25rem;
            }

            footer .top .middle img {
                height: 2.5rem;
            }

            footer .message {
                font-size: 1rem;
                line-height: 1.5rem;
            }

            footer .copy-right {
                font-size: 1rem;
                margin-top: 3rem;
            }
        }
