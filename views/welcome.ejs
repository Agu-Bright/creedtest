    <style>
        @import url('https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&family=Work+Sans:wght@200;300;400;500;600;700;800;900&display=swap');

        * {
            padding: 0;
            margin: 0;
            box-sizing: border-box;
        }

        body {
            /* background-color: white; */
            background-color: #1E1E1E;
        }

        .star-watermark {
            position: fixed;
            top: 40vh;
            height: 40vh;
            left: 0;
            object-fit: contain;
            overflow: visible;
            pointer-events: none;
            z-index: 0;
            opacity: 50%;
        }

        .logo {
            height: 4rem;
            object-fit: contain;
            margin-bottom: 1rem;
        }

        main {
            width: 90%;
            margin-left: auto;
            margin-right: auto;
            margin-top: 4px;
        }

        .hero {
            height: 30vh;
            object-fit: cover;
            border-radius: 0.25rem;
            border: 1px solid #DAA520;
            width: 100%;
            z-index: 3;
        }

        h1 {
            font-family: 'Work Sans', sans-serif;
            font-weight: bold;
            color: #fff;
            margin-top: 10px;
            font-size: 1.3rem;
        }

        .message {
            white-space: pre-line;
            font-family: 'Inter', sans-serif;
            font-weight: normal;
            color: #fff;
        }

        .cta {
            background-color: #DAA520;
            color: #ffffff;
            padding: 0.5rem 1rem;
            border-radius: 4px;
            margin: 3.5rem auto;
            display: block;
            width: fit-content;
            text-decoration: none;
            font-family: 'Inter', sans-serif;
            text-align: center;
        }

        footer {
            background-color: #455A64;
            border-radius: 0.75rem;
            position: relative;
            padding: 1rem;
            overflow: hidden;
        }

        footer>img {
            position: absolute;
            top: -1.5rem;
            left: -2.5rem;
            width: 48vw;
            pointer-events: none;
            object-fit: contain;
        }

        /* flex items-center justify-between flex-1 gap-2 */
        footer .top {
            display: flex;
            align-items: center;
            justify-content: space-between;
            gap: 0.5rem;
            flex-wrap: wrap;
        }

        footer .logo-container {
            flex: 1;
        }

        footer .flogo {
            width: 10rem;
            object-fit: contain;
            flex: 1;
            margin-bottom: 0;
        }

        /* lex items-center gap-3 lg:gap-5 */
        footer .top .middle {
            display: flex;
            align-items: center;
            gap: 0.875rem;
            flex: 1;
            justify-content: center;
        }

        /* h-8 object-contain lg:h-10 */
        footer .top .middle img {
            height: 2rem;
            object-fit: contain;
        }

        footer .top .faq {
            font-family: 'Inter', sans-serif;
            color: #fff;
            font-weight: 600;
            flex: 1;
            text-align: left;
        }

        footer .message {
            color: #fff;
            margin-top: 1.25rem;
            text-align: center;
            font-size: 0.875rem;
            white-space: pre-line;
            font-family: 'Inter', sans-serif;
            font-weight: 400;
        }

        footer .message a {
            color: #DAA520;
        }

        footer .copy-right {
            color: #DAA520;
            font-size: 0.875rem;
            margin-top: 1rem;
            font-family: 'Inter', sans-serif;
        }

        @media (min-width: 640px) {
            .cta {
                font-size: 1rem;
                padding: 1rem 2rem;
            }

            footer .top .faq {
                text-align: right;
            }
        }

        @media (min-width: 768px) {
            footer .top {
                width: 100%;
            }
/* 
            .flogo {
